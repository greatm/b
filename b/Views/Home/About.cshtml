@model IEnumerable<whatsnew>

@{
    ViewBag.Title = "About";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>

<article>
    <p>
        we make life easier.
    </p>
    create <b>custom</b> software.


    <form action="http://www.bing.com/search" method="post">
        <input name="q" type="text" />
        <input type="submit" value="Search!" />
    </form>

</article>

<aside>
    <h3>What's new </h3>

    @foreach (var whats in Model)
    { 
        <p>@whats.WorkTime
            <br />
            @whats.Work
        </p>
    }
</aside>
