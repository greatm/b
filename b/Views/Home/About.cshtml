@model IEnumerable<b.Models.whatsnew>

@{
    ViewBag.Title = "About";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>

<article>
    <p>
        we make life easier.
    </p>
    create <b>custom</b> software.

    <input id="helloButton1" value="Click Me" onclick="doSomething();">
    <script type="text/javascript">
        function doSomething() {
            alert("hello events!");
        }
    </script>



    <input id="helloButton" value="Click Me">
    <script type="text/javascript">
        function doSomething() {
            alert("hello events!");
        }
        $(function () {
            $("#helloButton").click(doSomething);
        });
    </script>

</article>

<aside>
    <h3>What's new </h3>

    @foreach (var whats in Model)
    { 
        <p>@whats.WorkTime
            <br />
            @whats.Work
        </p>
    }
</aside>
