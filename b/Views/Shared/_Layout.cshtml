<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>b @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />

    @Styles.Render("~/bundles/css")
    <link rel="stylesheet" href="~/Content/themes/base/jquery-ui.css" type="text/css" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="~/Content/jquery.jgrowl.min.css" />
    <script src="~/Scripts/jquery.jgrowl.min.js"></script>

    <script type="text/javascript" src="~/Content/jquery.jqGrid.min.js"></script>
    @*<script type="text/javascript" src="jquery-ui-1.10.0.js"></script>*@
    <link href="~/Scripts/ui.jqgrid.css" type="text/css" rel="Stylesheet" />


    <script src="~/Scripts/MyScript.js"></script>
    @Scripts.Render("~/bundles/ajax")
</head>
<body>
    <header>
        <script type="text/javascript">m("u r in: @ViewBag.Title ");</script>
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">@Html.ActionLink("b", "Index", "Home", null, new { @class = "site-logo" })</p>
            </div>
            <div class="float-right">
                <div class="status-message"></div>
                <section id="login">
                    @Html.Partial("_LoginPartial")
                </section>
            </div>
        </div>
    </header>
    <div id="accordionMenu">
        <h3>Home</h3>
        <div>
            <p>@Html.ActionLink("Home", "Index", "Home")</p>
            <p>@Html.ActionLink("About", "About", "Home")</p>
            <p>@Html.ActionLink("Contact", "Contact", "Home")</p>
        </div>
        <h3>Master</h3>
        <div>
            <p>@Html.ActionLink("Vendor", "Index", "MasterVendor")</p>
            <p>@Html.ActionLink("Product", "Index", "MasterProduct")</p>
            <hr />
            <p>@Html.ActionLink("Store", "Index", "MasterStore")</p>
            <p>@Html.ActionLink("Sub Location", "Index", "MasterSublocation")</p>
            <hr />
            <p>@Html.ActionLink("Customers", "Index", "MasterCustomer")</p>
        </div>
        <h3>Purchase</h3>
        <div>
            <p>@Html.ActionLink("Purchase Order", "Index", "PurchaseOrder")</p>
            <p>@Html.ActionLink("Purchase", "Index", "Purchase")</p>
        </div>
        <h3>Sales</h3>
        <div>
            <p>@Html.ActionLink("Sales Order", "Index", "SalesOrder")</p>
            <p>@Html.ActionLink("Sales", "Index", "Sales")</p>
            <p>@Html.ActionLink("Packing List", "Index", "PackingList")</p>
        </div>
        <h3>Reports</h3>
        <div>
            <p>@Html.ActionLink("Users", "ViewReport", "ReportUsers")</p>
            <hr />
            <p>@Html.ActionLink("Purchase", "ViewReport", "ReportPurchase")</p>
            <p>@Html.ActionLink("Sales", "ViewReport", "ReportSales")</p>
        </div>
    </div>
    <div id="body">
        @RenderSection("featured", required: false)
        <section class="content-wrapper main-content clear-fix">
            @* @(Html.Syncfusion().StyleManager().Combine(true).Minify(true)
            .Register(component =>
            {
                component.Add(ComponentType.Grid);
            }))

            @{Html.Syncfusion().ScriptManager().Combine(true).Minify(true).Render();}*@

            @RenderBody()
        </section>
    </div>

    <footer>
        <div class="content-wrapper">
            <div class="float-left">
                <p>&copy; @DateTime.Now.Year - great.</p>
            </div>
        </div>
    </footer>

    @RenderSection("scripts", required: false)
    @*@StackExchange.Profiling.MiniProfiler.RenderIncludes()*@
</body>
</html>
